{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },

  "env": {
    "es6": true
  }
}
